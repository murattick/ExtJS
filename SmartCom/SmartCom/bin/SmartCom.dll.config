        <add name="CustomMembershipProvider" type="SmartCom.Security.MyMembershipProvider" connectionStringName="CustomMembershipDB" enablePasswordRetrieval="false" requiresQuestionAndAnswer="false" requiresUniqueEmail="false" />
      </providers>
    </membership>
    <roleManager enabled="true" defaultProvider="CustomRoleProvider">
      <providers>
        <add name="DefaultRoleProvider" type="System.Web.Providers.DefaultRoleProvider, System.Web.Providers, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" connectionStringName="CustomMembershipDB" applicationName="/" />
        <clear />
        <add name="CustomRoleProvider" type="SmartCom.Security.MyRoleProvider" connectionStringName="CustomMembershipDB" applicationName="/" />
      </providers>
    </roleManager>
    <sessionState mode="InProc" customProvider="DefaultSessionProvider">
      <providers>
        <add name="DefaultSessionProvider" type="System.Web.Providers.DefaultSessionStateProvider, System.Web.Providers, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" connectionStringName="CustomMembershipDB" />
